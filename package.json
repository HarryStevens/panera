{
	"name": "panera",
	"version": "0.0.6",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "pnpm run format:check && eslint .",
		"test:unit": "vitest",
		"test": "pnpm run -s test:unit -- --run",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prepublishOnly": "pnpm run lint && pnpm run test",
    "preversion": "pnpm run lint && pnpm run test"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^4 || ^5"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@storybook/addon-svelte-csf": "^5.0.7",
		"@storybook/sveltekit": "^9.1.2",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/node": "^22",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-storybook": "^9.1.2",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"storybook": "^9.1.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3"
	},
	"keywords": [
		"svelte"
	],
	"engines": {
		"node": ">=18"
	},
	"packageManager": "pnpm@10.14.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"repository": "github:harrystevens/panera",
	"homepage": "https://panera-two.vercel.app/",
	"bugs": {
		"url": "https://github.com/harrystevens/panera/issues"
	}
}
